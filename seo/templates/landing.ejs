  <script>
    // Typewriter effect
    const rotatingQuestions = [
      "should I quit?",
      "is this fixable?",
      "what do I even want?",
      "how do I say this?",
      "am I overreacting?",
      "is it worth the risk?"
    ];
    
    let phraseIndex = 0;
    let charIndex = 0;
    let isDeleting = false;
    const typewriter = document.getElementById('typewriter');
    
    function type() {
      const currentPhrase = rotatingQuestions[phraseIndex];
      
      if (isDeleting) {
        charIndex--;
        typewriter.textContent = currentPhrase.substring(0, charIndex);
        
        if (charIndex === 0) {
          isDeleting = false;
          phraseIndex = (phraseIndex + 1) % rotatingQuestions.length;
          return setTimeout(type, 300);
        }
        
        return setTimeout(type, 40);
      } else {
        charIndex++;
        typewriter.textContent = currentPhrase.substring(0, charIndex);
        
        if (charIndex === currentPhrase.length) {
          return setTimeout(function() {
            isDeleting = true;
            type();
          }, 2000);
        }
        
        return setTimeout(type, 80);
      }
    }
    
    type();
    
    // FAQ accordion
    document.querySelectorAll('.sp-landing-faq-question').forEach(function(button) {
      button.addEventListener('click', function() {
        const item = this.closest('.sp-landing-faq-item');
        const isOpen = item.classList.contains('sp-landing-faq-open');
        
        // Close all
        document.querySelectorAll('.sp-landing-faq-item').forEach(function(i) {
          i.classList.remove('sp-landing-faq-open');
          i.querySelector('.sp-landing-faq-icon').textContent = '+';
          i.querySelector('.sp-landing-faq-question').setAttribute('aria-expanded', 'false');
        });
        
        // Open clicked one if it was closed
        if (!isOpen) {
          item.classList.add('sp-landing-faq-open');
          this.querySelector('.sp-landing-faq-icon').textContent = '-';
          this.setAttribute('aria-expanded', 'true');
        }
      });
    });
    
    // Capture promo code from URL
    (function() {
      var urlParams = new URLSearchParams(window.location.search);
      var promoCode = urlParams.get('promo');
      if (promoCode) {
        sessionStorage.setItem('sp_promo_code', promoCode);
      }
    })();
  </script>
  
